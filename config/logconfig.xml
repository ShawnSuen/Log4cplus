<?xml version="1.0" encoding="utf-8"?>

<!--log文件配置文件-->
<Init>
  <!--设置log文件保存路径-->
  <FilePath>../log</FilePath>
  <!-- 设置log文件上限-->
  <FileNum>5</FileNum>
  <!-- 设置log文件大小上限-->
  <FileSize>5M</FileSize>
  <!--设置是否将log日志写到文件内-->
  <Write2File>true</Write2File>
  <!--设置是否将log日志输出到控制台 默认为开启状态-->
  <Write2Console>false</Write2Console>
</Init>
